{
    "manifest_version": 2,
    "name": "SOSUV Send Data to Google Sheets",
    "version": "0.111",
    "description": "Testing sending data to Google Sheets",
    "content_scripts": [{
      "matches": ["https://github.com/*","https://api.github.com/*/*","https://script.google.com/*/*/*"],
      "js": ["content.js"]
    }],
    "background" : {
      "scripts" : ["background.js","githubAPI.js"]
    },
    "icons": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  }
